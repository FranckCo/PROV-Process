@prefix ex-act: <http://example.org/linkage/activity/> .
@prefix ex:    <http://example.org/linkage/> .
@prefix ex-prod: <http://example.org/linkage/product/> .

# Source data sets to source index data sets

# Should be a prov:Collection? (same question for all the entities)
ex-prod:sourceDatasets a prov:Entity ;
        rdfs:label "Source datasets"@en .

# dcterms:isPartOf should be discussed
# Lineage metadata would be attached to the prov:Derivation instance
ex-prod:sourceIndexDatasets a prov:Entity ;
        rdfs:label "Source index datasets"@en ;
        prov:wasDerivedFrom ex-prod:sourceDatasets ;
        dcterms:isPartOf ex-prod:sourceDatasets ;
        prov:qualifiedDerivation [
             a prov:Derivation ;
             prov:entity ex-prod:sourceDatasets ;
             prov:hadActivity ex-act:subsetDatasetsLinkageVariables ;
        ] .

ex-act:subsetDatasetsLinkageVariables a prov:Activity ;
        rdfs:label "Subset datasets linkage variables"@en .